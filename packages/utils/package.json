{
  "name": "@pianduan/vue-i18n-command-utils",
  "version": "0.0.4",
  "description": "vue 项目 i18n 辅助工具",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "publish": "pnpm publish --no-git-checks --access public"
  },
  "keywords": [
    "vue",
    "i18n",
    "vue-i18n"
  ],
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pianduan-M/vueI18n-command"
  },
  "author": "pianduan",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.26.1",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.26.0",
    "@vue/compiler-sfc": "^3.5.12",
    "axios": "^1.7.7",
    "crypto": "^1.0.1",
    "ignore": "^6.0.2",
    "lodash": "^4.17.21",
    "node-xlsx": "^0.24.0",
    "prettier": "^3.3.3",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "rollup-plugin-copy": "^3.5.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^4.25.0"
  },
  "publishConfig": {
    "linkWorkspacePackages": false
  }
}